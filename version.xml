<package name="TypeLinked" version="0.0.1">
    <depends on='BEAST.base' atleast='2.7.6'/>
    <depends on='BEAST.app' atleast='2.7.6'/>
    <depends on='BEAST_CLASSIC' atleast='1.6.2'/>

    <service type="beast.base.evolution.datatype.DataType">
        <provider classname="beast.base.evolution.datatype.Nucleotide"/>
    </service>

    <service type="beast.base.core.BEASTInterface">
        <provider classname="beast.base.evolution.branchratemodel.TraitLinkedClockModel"/>
        <provider classname="beast.base.evolution.branchratemodel.TraitLinkedInstantSwitchStrictClockModel"/>
        <provider classname="beast.base.evolution.branchratemodel.TraitLinkedExpectedOccupancyStrictClockModel"/>
        <provider classname="beast.base.evolution.branchratemodel.OccupancyLinkedClockModel"/>
        <provider classname="beast.base.evolution.likelihood.AncestralSampledStateTreeLikelihood"/>
        <provider classname="beast.base.evolution.likelihood.AncestralStateMinimumSwitchTreeLikelihood"/>
        <provider classname="beast.base.evolution.substitutionmodel.SVSGeneralSubstitutionModelNew"/>
        <provider classname="beast.base.evolution.operator.TraitAndOccupancyTreeOperator"/>
    </service>
</package>