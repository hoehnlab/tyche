<package name="TyCHE" version="0.0.0-alpha.5">
    <depends on='BEAST.base' atleast='2.7.6'/>
    <depends on='BEAST.app' atleast='2.7.6'/>
    <depends on='BEAST_CLASSIC' atleast='1.6.2'/>

    <service type="beast.base.evolution.datatype.DataType">
        <provider classname="beast.base.evolution.datatype.Nucleotide"/>
    </service>

    <service type="beast.base.core.BEASTInterface">
        <provider classname="beast.base.evolution.branchratemodel.TypeLinkedInstantSwitchStrictClockModel"/>
        <provider classname="beast.base.evolution.branchratemodel.TypeLinkedExpectedOccupancyClockModel"/>
        <provider classname="beast.base.evolution.likelihood.AncestralTypeLikelihood"/>
        <provider classname="beast.base.evolution.substitutionmodel.TycheSVSGeneralSubstitutionModel"/>
        <provider classname="beast.base.evolution.operator.LeafConsciousTypeTreeOperator"/>
        <provider classname="beast.base.evolution.operator.SubtreeTypeSwitchOperator"/>
        <provider classname="beast.base.inference.distribution.ElementwisePrior"/>
    </service>
</package>