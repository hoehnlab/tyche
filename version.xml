<package name="TyCHE" version="0.0.1-beta.1">
    <depends on='BEAST.base' atleast='2.7.6'/>
    <depends on='BEAST.app' atleast='2.7.6'/>
    <depends on='BEAST_CLASSIC' atleast='1.6.2'/>

    <service type="beast.base.evolution.datatype.DataType">
        <provider classname="beast.base.evolution.datatype.Nucleotide"/>
    </service>

    <service type="beast.base.core.BEASTInterface">
        <provider classname="tyche.evolution.branchratemodel.TycheInstantSwitchClockModel"/>
        <provider classname="tyche.evolution.branchratemodel.TycheExpectedOccupancyClockModel"/>
        <provider classname="tyche.evolution.likelihood.AncestralTypeLikelihood"/>
        <provider classname="tyche.evolution.substitutionmodel.TycheSVSGeneralSubstitutionModel"/>
        <provider classname="tyche.evolution.operator.LeafConsciousTypeTreeOperator"/>
        <provider classname="tyche.evolution.operator.SubtreeTypeSwitchOperator"/>
        <provider classname="tyche.inference.distribution.ElementwisePrior"/>
    </service>
</package>